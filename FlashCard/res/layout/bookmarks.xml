<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/LinearLayout02" android:layout_width="fill_parent"
	android:layout_height="wrap_content" android:background="@drawable/port_bg"
	android:padding="10sp">
	<TableLayout android:id="@+id/LinearLayout01"
		android:layout_width="wrap_content" android:layout_height="wrap_content" android:background="@drawable/prefbg">
		<TextView android:id="@+id/tvTitle" android:layout_height="wrap_content"
			android:background="@drawable/resultshdr" android:text="@string/bookmarkhead"
			android:textColor="#fff" android:textSize="18sp" android:paddingTop="6sp"
			android:gravity="center_horizontal" android:layout_margin="10sp"
			android:layout_width="fill_parent"></TextView>
		<ListView android:id="@+id/bookmarkList"
			android:layout_height="fill_parent" android:layout_weight="2"
			android:layout_gravity="fill_vertical" android:layout_width="fill_parent"
			android:fadingEdge="vertical" android:fadingEdgeLength="50.0sp"
			android:fitsSystemWindows="true" android:scrollbars="none"
			android:cacheColorHint="#00000000" android:layout_marginRight="5sp">
		</ListView>
		
	
</TableLayout>
</LinearLayout>
