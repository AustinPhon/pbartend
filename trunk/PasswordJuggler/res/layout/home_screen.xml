<RelativeLayout android:id="@+id/RelativeLayout01"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:background="@drawable/base_bg" android:layout_gravity="top">
	<TableLayout android:layout_width="fill_parent" android:layout_height="fill_parent" android:id="@+id/tvMainView">
		<LinearLayout android:id="@+id/LinearLayout02"
			android:layout_height="wrap_content" android:background="@drawable/nav_bg"
			android:layout_width="fill_parent">
		</LinearLayout>
		<TableLayout android:id="@+id/TableLayout01"
			android:background="@drawable/list_item"
			android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_margin="15sp">
			<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:background="@drawable/item_spacer" android:gravity="center_vertical" android:paddingLeft="20sp">
				<TextView android:id="@+id/TextView01"
					android:layout_width="wrap_content" android:layout_height="wrap_content" android:textColor="#000" android:text="Login"></TextView>
			</TableRow>
		</TableLayout>


</TableLayout>
	<LinearLayout android:id="@+id/LinearLayout01"
		android:layout_height="wrap_content" android:layout_width="fill_parent"
		android:background="@drawable/footer_nav"
		android:layout_alignParentBottom="true"></LinearLayout>
</RelativeLayout>
